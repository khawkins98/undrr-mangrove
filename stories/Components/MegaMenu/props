type MegaMenu = {
  sections: MegaMenuSection[]
}

type MegaMenuSection = {
  title: string
  bannerHeading: string
  bannerDescription: string
  items: MegaMenuItem[]
}

type MegaMenuItem = {
  title: string
  items: MegaMenuLink[]
}

type MegaMenuLink = {
  title: string
  url: string
  children?: MegaMenuLink[]
}